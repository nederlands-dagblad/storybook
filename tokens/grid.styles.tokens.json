{
  "Layout grid (<xl)": {
    "$type": "grid",
    "$value": [
      {
        "pattern": "columns",
        "visible": true,
        "alignment": "stretch",
        "color": "#ff00001a",
        "gutterSize": "{layout.gutter}",
        "count": "{layout.columns}",
        "offset": "{layout.padding}"
      }
    ]
  }
}